{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.2",
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/favicon.ico",
            "headers": {
                "Cache-Control": "public, max-age=31556952, immutable"
            },
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/robots.txt",
            "headers": {
                "Cache-Control": "public, max-age=31556952, immutable"
            },
            "dest": "/public/robots.txt"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
